Recrafted by Mishamol"
run/main_scene="res://levels/game_scene.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="cool_cyan_donut/kitchen_chaos"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://assets/app_icons/bread_icon.png"

[autoload]

ApplicationControls="*res://scripts/application_controls.gd"
Singleton="*res://scripts/singleton.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1084
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="KitchenChaos"

[editor_plugins]

enabled=PackedStringArray("res://addons/phantom_camera/plugin.cfg", "res://addons/unidot_importer/plugin.cfg")

